{
    "name": "dbhcomponents",
    "version": "3.3.16",
    "description": "module with react-web-components for dbh",
    "license": "UNLICENSED",
    "main": "index.js",
    "types": "index.d.ts",
    "scripts": {
        "build": "npm run clean && npm run build:types && npm run build:tscjs && npm run bundle:tsc && npm run copy",
        "build:types": "tsc --emitDeclarationOnly --project ./tsconfig.build.json",
        "prebuild:types": "echo [Buidling *.d.ts]",
        "build:tscjs": "tsc --project ./tsconfig.build.json",
        "prebuild:tscjs": "echo [Building *.js]",
        "clean": "rimraf dist && rimraf build",
        "preclean": "echo [Cleaning build directories]",
        "copy": "copyfiles package.json dist && copyfiles README.md dist && copyfiles -u 1 src/assets/*.* dist",
        "precopy": "echo [Copying Package.json and README.md]",
        "bundle:tsc": "webpack --config webpack.config.prod.js",
        "prebundle:tsc": "echo [Building Bundles with Webpack]",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage --runInBand",
        "test-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
        "start": "webpack serve",
        "start-https": "webpack serve --https --key ./key.pem --cert ./cert.pem",
        "predeploy": "npm run clean && npm run bundle:tsc && copy _redirects build",
        "deploy": "netlify deploy --dir=build --prod",
        "build-modul": "npm run clean && npm run build:types && npm run build:tscjs && npm run copy",
        "polish-modul": "rimraf dist/example && rimraf dist/tsconfig.build.tsbuildinfo",
        "publish-modul-old": "npm run build-modul && npm run polish-modul && npm publish dist",
        "publish-modul": "npm run build-modul && npm run polish-modul && cd dist && npm publish",
        "to-NCTSZE": "npm run build-modul && npm run polish-modul && copyfiles -u 1 dist/**/*.* ../ac-web-nctsze/node_modules/dbhcomponents",
        "to-NCTSZE-SCAN": "npm run build-modul && npm run polish-modul && copyfiles -u 1 dist/**/*.* ../ac-web-nctsze-scan/node_modules/dbhcomponents",
        "version": "$npm_package_version",
        "clear-jest-cache": "jest --clearCache"
    },
    "author": {
        "name": "dbh"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "sideEffects": [
        "*.css"
    ],
    "dependencies": {
        "@date-io/date-fns": "1.3.13",
        "@dnd-kit/core": "6.0.6",
        "@dnd-kit/sortable": "7.0.1",
        "@emotion/react": "11.10.5",
        "@emotion/styled": "11.10.5",
        "@ericblade/quagga2": "1.7.7",
        "@mui/icons-material": "5.11.0",
        "@mui/lab": "5.0.0-alpha.111",
        "@mui/material": "5.11.0",
        "@mui/x-date-pickers": "5.0.10",
        "@types/big.js": "6.1.6",
        "@types/chart.js": "2.9.37",
        "@types/node": "18.11.12",
        "@types/react": "18.0.26",
        "@types/react-dom": "18.0.9",
        "@types/react-grid-layout": "1.3.2",
        "@types/react-router-dom": "5.3.3",
        "@types/react-virtualized": "9.21.21-rc.1",
        "@types/ua-parser-js": "0.7.36",
        "@types/uniqid": "5.3.2",
        "allotment": "1.17.0-rc.1",
        "big.js": "6.2.1",
        "chart.js": "3.8.2",
        "copy-webpack-plugin": "11.0.0",
        "copyfiles": "2.4.1",
        "css-loader": "6.7.2",
        "date-fns": "2.29.3",
        "fork-ts-checker-webpack-plugin": "7.2.13",
        "framebus": "5.2.0",
        "html-webpack-plugin": "5.5.0",
        "jspdf": "2.5.1",
        "jspdf-autotable": "3.5.28",
        "re-resizable": "6.9.9",
        "react": "18.2.0",
        "react-chartjs-2": "4.3.1",
        "react-content-loader": "6.2.0",
        "react-cookie": "4.1.1",
        "react-custom-scrollbars-2": "4.5.0",
        "react-dom": "18.2.0",
        "react-draggable": "4.4.5",
        "react-dropzone": "14.2.3",
        "react-error-boundary": "3.1.4",
        "react-grid-layout": "1.3.4",
        "react-router-dom": "5.3.3-rc.1",
        "react-toastify": "9.1.1",
        "react-virtualized": "9.22.3-rc.4",
        "rimraf": "3.0.2",
        "style-loader": "3.3.1",
        "terser-webpack-plugin": "5.3.6",
        "ts-loader": "9.4.2",
        "typescript": "4.9.4",
        "ua-parser-js": "1.0.32",
        "uniqid": "5.4.0",
        "webpack": "5.75.0",
        "webpack-cli": "5.0.1",
        "webpack-dev-server": "4.11.1"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "13.4.0",
        "@testing-library/user-event": "14.4.3",
        "@types/jest": "29.2.4",
        "jest": "29.3.1",
        "jest-environment-jsdom": "29.3.1",
        "prettier": "2.8.1",
        "ts-jest": "29.0.3"
    }
}
